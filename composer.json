{
    "name": "darkbox/cakephp-fuse",
    "description": "Cakephp 4 plugin that implements fuzzy search based on Fuse",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Liqueur de Toile",
            "homepage": "https://liqueurdetoile.com"
        },
        {
            "name": "RGM",
            "homepage": "https://github.com/darkbox/"
        }
    ],
    "keywords": [
        "database",
        "orm",
        "fuzzy",
        "fuse",
        "filter"
    ],
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan analyse"
    },
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4",
        "loilo/fuse": "^6.4",
        "adbario/php-dot-notation": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.12.55"
    },
    "autoload": {
        "psr-4": {
            "Lqdt\\CakephpFuse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lqdt\\CakephpFuse\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}